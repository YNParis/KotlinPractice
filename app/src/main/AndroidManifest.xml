<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.demos.yxn.lifecircle">

    <application
        android:name=".application.MyApp"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        tools:ignore="GoogleAppIndexingWarning">
        <!--开启页面-->
        <activity android:name=".Activity.OpenActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".ipc.IPCActivity">
        </activity>

        <service
            android:name=".ipc.RemoteService"
            android:process=":remote">
        </service>

        <activity android:name=".webview.WebViewActivity">
        </activity>
        <activity
            android:name=".Activity.MainActivity"
            android:configChanges="orientation|screenSize">
        </activity>
        <activity
            android:name=".Activity.BActivity"
            android:configChanges="orientation|screenSize"
            android:launchMode="singleInstance"/>
        <activity
            android:name=".Activity.AActivity"
            android:launchMode="singleTask"/>
        <activity
            android:name=".Activity.CActivity"
            android:launchMode="singleTop"
            android:theme="@style/Theme.AppCompat.Dialog"/>
        <activity
            android:name=".Activity.DActivity"
            android:launchMode="standard"/>
        <activity
            android:name=".RecyclereViewDemo.RcviewActivity"
            android:launchMode="singleTask"/>
        <activity android:name=".Activity.NdkDemoActivity">
        </activity>
        <activity android:name=".Activity.ToolbarActivity"/>
        <activity android:name=".kotlin.Main3Activity">
        </activity>
        <activity android:name=".ijkdemo.PlayerTest"/>
    </application>

</manifest>