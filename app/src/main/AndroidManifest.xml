<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.demos.kotlin">

    <application
        android:name=".lifecycle.MyApp"
        android:allowBackup="true"
        android:icon="@drawable/icon"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        tools:ignore="GoogleAppIndexingWarning">
        <activity android:name=".views.DrawerActivity"></activity>
        <activity android:name=".views.TouchableBallActivity"></activity> <!-- 开启页面 -->
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".webview.WebViewActivity"></activity>
        <activity
            android:name=".lifecycle.FirstActivity"
            android:configChanges="orientation|screenSize"></activity>
        <activity
            android:name=".lifecycle.BActivity"
            android:configChanges="orientation|screenSize"
            android:launchMode="singleInstance" />
        <activity
            android:name=".lifecycle.AActivity"
            android:launchMode="singleTask" />
        <activity
            android:name=".lifecycle.CActivity"
            android:launchMode="singleTop"
            android:theme="@style/Theme.AppCompat.Dialog" />
        <activity
            android:name=".lifecycle.DActivity"
            android:launchMode="standard" />
        <activity android:name=".ndk.NdkDemoActivity"></activity>
        <activity android:name=".kotlinsyntax.KotlinSyntaxActivity"></activity>
        <activity android:name=".ijk.PlayerTest" />
    </application>

</manifest>