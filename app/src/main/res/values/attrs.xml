<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="CommonChartView">
        <attr name="chartType" format="string|reference" />

        <attr name="valueTextColor" format="color|reference" />
        <attr name="valueTextSize" format="dimension|reference" />

        <attr name="labelTextColor" format="color|reference" />
        <attr name="labelTextSize" format="dimension|reference" />

        <attr name="gridColor" format="color|reference" />
        <attr name="gridWidth" format="dimension|reference" />
    </declare-styleable>


    <declare-styleable name="RangeFilterBar">
        <attr name="color_line_normal" format="color" />
        <attr name="color_line_select" format="color" />
        <attr name="stroke_width_normal" format="dimension" />
        <attr name="stroke_width_select" format="dimension" />
        <attr name="progress_icon" format="reference" />
        <attr name="text_size" format="dimension" />
        <attr name="text_color" format="color" />
    </declare-styleable>
</resources>